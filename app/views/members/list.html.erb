<% @members.each do |member| %>

  <%= member.full_name %>
  <%= l(member.birthday, format: :long) if public?(member, "birthday") %>
  <%= member.email if public?(member, "email") %>

  <% member.phones.each do |phone| %>

    <% unless phone.private %>
      <%= t("phone.phone_types.#{phone.type}") %>
      <%= phone.number %>
    <% end %>

  <% end %>

<% end %>