<div class="errors"><%= render 'error_messages', object: f.object %></div>

<div>
  <%= f.label :name do |label| %>
    <%= label %>
    <%= f.text_field :name %>
  <% end %>
</div>

<div>
  <%= f.label :group_type do |label| %>
    <%= label %>
    <%= f.select :group_type, options_for_enum(@group, :group_type) %>
  <% end %>
</div>

<div>
  <%= f.label :place do |label| %>
    <%= label %>
    <%= f.select :place, options_for_enum(@group, :place) %>
  <% end %>
</div>

<div>
  <%= f.label :place_other do |label| %>
    <%= label %>
    <%= f.text_field :place_other %>
  <% end %>
</div>

<div class="buttons">
  <%= f.button %>
</div>