<h1><%= t ".title" %></h1>

<%= form_for @event do |f| %>

  <%= render 'error_messages', object: f.object %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :dtstart %>
  <%= f.datetime_select :dtstart %>

  <%= f.label :dtend %>
  <%= f.datetime_select :dtend %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.label :url %>
  <%= f.text_field :url %>

  <%= f.submit %>

<% end %>